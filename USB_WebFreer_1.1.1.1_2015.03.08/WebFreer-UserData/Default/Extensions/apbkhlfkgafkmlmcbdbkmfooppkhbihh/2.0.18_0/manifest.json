{
   "background": {
      "scripts": [ "libs/promise-0.1.1.min.js", "libs/analiticsTracking.js", "libs/raven.min.js", "src/bg/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/play19.png",
      "default_title": "Play SoundCloud"
   },
   "commands": {
      "play-pause": {
         "description": "Play/Pause",
         "suggested_key": {
            "default": "Ctrl+Shift+A",
            "mac": "Command+Shift+A"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "libs/analiticsTracking.js", "src/inject/inject.js" ],
      "matches": [ "*://*.soundcloud.com/*", "*://soundcloud.com/*" ],
      "run_at": "document_idle"
   }, {
      "css": [ "src/player/player.css" ],
      "exclude_matches": [ "*://*.soundcloud.com/*", "*://soundcloud.com/*" ],
      "js": [ "src/player/player.js" ],
      "matches": [ "\u003Call_urls\u003E" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'; script-src 'self'",
   "description": "Extension allows you to play/pause your SoundCloud stream using button or on site player. Do it even faster by using context menu",
   "homepage_url": "http://quick-hint.com",
   "icons": {
      "128": "icons/soundcloud_btn_ext_icon_64x64.png",
      "16": "icons/soundcloud_btn_ext_icon_48x48.png",
      "48": "icons/soundcloud_btn_ext_icon_48x48.png"
   },
   "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDTyNiZR3qQoLyROnTpBKrWmU+06cWXCOelo1c95uHbhQ5lqAuSizlutNU6i240XvInEOYBBT4uXCfAdC0OMLsyfBfYz0ETu45basdmuqJ285TbfYcUIy0NkeI2WTezbB3qzbgOS8F4MO6klPZIWSP0WSrI6+d2Z6cfGzMLyb3MxQIDAQAB",
   "manifest_version": 2,
   "name": "SoundCloud Button",
   "options_page": "src/options/options.html",
   "permissions": [ "tabs", "contextMenus", "*://*.soundcloud.com/*", "*://soundcloud.com/*", "https://ssl.google-analytics.com/ga.js" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
